{
    "name": "subjective-php/psr-cache-helper",
    "description": "Utilities to assist with PSR-16 simple cache implementations",
    "keywords": ["psr", "psr16", "cache", "util", "trait"],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0||^8.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "SubjectivePHP\\Psr\\SimpleCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubjectivePHPTest\\Psr\\SimpleCache\\": "tests"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "test": "vendor/bin/phpunit"
    }
}
